{
    "name": "AussieShopper",
    "description": "Your personal shopper assistant",
    "version": "1.0.0",
    "author": "Alaa Salman",
    "license": "MIT",
    "private": true,
    "eslintConfig": {
        "parserOptions": {
            "ecmaVersion": 8,
            "sourceType": "module"
        },
        "extends": [
            "eslint:recommended",
            "plugin:vue/recommended"
        ],
        "plugins": [
            "vue",
            "import"
        ],
        "rules": {}
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
        "build-dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --watch --watch-poll=1000"
    },
    "dependencies": {
        "@fortawesome/fontawesome": "^1.1.3",
        "@fortawesome/fontawesome-free-solid": "^5.0.6",
        "axios": "^0.21.4",
        "bulma": "^0.7.1",
        "chart.js": "^2.7.1",
        "query-string": "^6.0.0",
        "vue": "^2.5.16",
        "vue-bulma-datepicker": "^1.3.6",
        "vue-chartjs": "^3.2.1",
        "webpack-bundle-tracker": "^0.3.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-3": "^6.24.1",
        "cross-env": "^5.1.4",
        "css-loader": "^0.28.11",
        "eslint": "^4.19.0",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-vue": "^4.3.0",
        "file-loader": "^1.1.11",
        "node-sass": "^7.0.1",
        "sass-loader": "^7.0.1",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "vue-loader": "^14.2.1",
        "vue-template-compiler": "^2.5.16",
        "webpack": "^4.6.0",
        "webpack-cli": "^2.0.12"
    }
}
